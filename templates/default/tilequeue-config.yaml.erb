aws:
  sqs:
    name: <%= node[:tilequeue][:aws][:sqs][:name] %>
    timeout-seconds: 20
  s3:
    bucket: <%= node[:tilequeue][:aws][:s3][:bucket] %>
    path: <%= node[:tilequeue][:aws][:s3][:path] %>
    reduced-redundancy: True
  credentials:
    aws_access_key_id: <%= node[:tilequeue][:aws][:credentials][:aws_access_key_id] %>
    aws_secret_access_key: <%= node[:tilequeue][:aws][:credentials][:aws_secret_access_key] %>
tiles:
  metro-extract:
    url: https://raw.githubusercontent.com/mapzen/metroextractor-cities/master/cities.json
    zoom-filter: <%= node[:tilequeue][:tiles][:metro_extract][:zoom_filter] %>
  top-tiles:
    url: https://gist.github.com/brunosan/b7ce3df8b48229a61b5b/raw/37e42e77f253bc204076111c92acc4d5e653edd2/top_50k_tiles.csv
    zoom-start: <%= node[:tilequeue][:tiles][:top_tiles][:zoom_start] %>
    zoom-until: <%= node[:tilequeue][:tiles][:top_tiles][:zoom_until] %>
  unique: true
  zoom-start: <%= node[:tilequeue][:tiles][:zoom_start] %>
  zoom-until: <%= node[:tilequeue][:tiles][:zoom_until] %>
  daemon: true
  expired-location: <%= node[:tilequeue][:tiles][:expired_location] %>
workers: <%= node[:tilequeue][:workers] %>
messages_at_once: <%= node[:tilequeue][:messages_at_once] %>
tilestache:
  config: <%= node[:tilequeue][:tilestache][:config] %>
  formats: <%= node[:tilequeue][:tilestache][:formats] %>
logging:
  config: <%= node[:tilequeue][:logging][:config] %>
redis:
  host: <%= node[:tilequeue][:tilediff][:redis][:host] %>
  port: <%= node[:tilequeue][:tilediff][:redis][:port] %>
  db: <%= node[:tilequeue][:tilediff][:redis][:db] %>
  cache-set-key: <%= node[:tilequeue][:tilediff][:redis][:cache_set_key] %>
postgresql:
  host: <%= node[:tilequeue][:postgresql][:host] %>
  port: 5432
  dbnames: <%= node[:tilequeue][:postgresql][:dbnames] %>
  user: <%= node[:tilequeue][:postgresql][:user] %>
  password: <%= node[:tilequeue][:postgresql][:password] %>
